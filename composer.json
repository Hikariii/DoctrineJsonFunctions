{
	"name": "syslogic/doctrine-json-functions",
	"type": "library",
	"description": "A set of extensions to Doctrine 2 that add support for json query functions.",
	"keywords": ["doctrine", "orm", "json", "dql", "database"],
	"license": "MIT",
	"authors": [
		{
			"name": "Doctrine Json Functions Contributors",
			"homepage": "https://github.com/SyslogicNL/DoctrineJsonFunctions/contributors"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"doctrine/orm": ">=2.2.3"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.5",
		"doctrine/data-fixtures": "^1.0",
		"symfony/yaml": "2.*",
		"doctrine/orm": "~2.1",
		"squizlabs/php_codesniffer": "2.8.*",
		"ext-pdo_sqlite": "*"
	},
	"autoload": {
		"psr-4": {
			"Syslogic\\DoctrineJsonFunctions\\" : "src/"
		}
	},
	"autoload-dev": {
		"psr-4": { "Syslogic\\DoctrineJsonFunctions\\Tests\\": "tests/" }
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
